<!DOCTYPE html>
<html lang="en">

<%- include("../header"); -%>  <!-- 불러오기 -->

<body> 
  <%- include("../menu"); -%>


  
  <button type="button" class="btn btn-primary" onclick="location.href='/api/music/new'">추가</button>

<!-- Card deck -->
<div class="row row-cols-1 row-cols-md-4" >

    <% result.forEach(music => { %>
        
    <!-- Card -->
    <div class="card mb-4">
  
      <!--Card image-->
      <div class="view overlay">
        <img class="card-img-top" src="https://mdbootstrap.com/img/Photos/Others/images/16.jpg"
          alt="Card image cap">
        <a href="#!">
          <div class="mask rgba-white-slight"></div>
        </a>
      </div>
  
      <!--Card content-->
      <div class="card-body">
  
        <!--Title-->
        <h4 class="card-title"><%= music.singer %></h4>
        <!--Text-->
        <p class="card-text"><%= music.title %></p>
        <!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
        <button type="button" class="btn btn-light-blue btn-md" onclick="location.href='/api/music/id/<%=music._id%>'">Read more</button>
  
      </div>
  
    </div>
    <!-- Card -->
  
    <% }) %>
  </div>
  <!-- Card deck -->



  <%- include("../footer.ejs"); -%>
</body>
</html>
